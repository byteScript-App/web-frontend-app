{
	"name": "web-frontend-app",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "bunx --bun vite",
		"build": "bunx --bun vite build",
		"dev:vite:node": "vite",
		"build:vite:node": "tsc -b && vite build",
		"test": "bun test",
		"test:with:report": "bun test --reporter=junit --reporter-outfile=./bun.xml",
		"check": "bunx biome check ./src",
		"check:fix": "bunx biome check --write ./src",
		"format": "bunx biome format ./src",
		"format:fix": "bunx biome format --write ./src",
		"lint": "bunx biome lint ./src",
		"lint:fix": "bunx biome lint --write ./src",
		"preview": "vite preview"
	},
	"dependencies": {
		"@reduxjs/toolkit": "^2.5.1",
		"i18next": "^24.2.2",
		"i18next-browser-languagedetector": "^8.0.2",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-i18next": "^15.4.0",
		"react-redux": "^9.2.0",
		"react-router": "^7.1.5",
		"redux-persist": "^6.0.0",
		"redux-persist-transform-encrypt": "^5.1.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@commitlint/cli": "^19.7.1",
		"@commitlint/config-conventional": "^19.7.1",
		"@commitlint/types": "^19.5.0",
		"@happy-dom/global-registrator": "^17.0.0",
